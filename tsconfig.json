{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "declaration": false,
    "outDir": ".",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
